package dev.carbajal.models;

public abstract class Account {
	
	int accountNum;
	String accountStatus;
	String accountType;
	double accountBal;
	
	public Account() {
		
	}
	
	// Constructor
	
	public Account(int accountNum, String accountStatus, String accountType, double accountBal) {
		
		this.accountNum = accountNum;
		this.accountType = accountType;
		this.accountBal = accountBal;
	}
	
	// Getters and Setters

	public int getAccountNumber() {
		return accountNum;
	}

	public void setAccountNumber(int accountNum) {
		this.accountNum = accountNum;
	}

	public String getAccountType() {
		return accountType;
	}

	public void setAccountType(String accountType) {
		this.accountType = accountType;
	}

	public double getAccountBal() {
		return accountBal;
	}

	public void setAccountBal(double accountBal) {
		this.accountBal = accountBal;
	}

	@Override
	public String toString() {
		return "Account [accountNum = " + accountNum + ", accountType = " + accountType + ", accountBal = " + accountBal
				+ "]";
	}
	
}
